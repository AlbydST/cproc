export
function w $main() {
@start.1
	%.1 =l alloc4 4
	%.2 =l alloc8 8
	%.3 =l alloc4 4
@body.2
	storew 0, %.1
	jnz 0, @cond_true.3, @cond_false.4
@cond_true.3
	jmp @cond_join.5
@cond_false.4
	%.4 =w loadw %.1
	storew %.4, %.3
@cond_join.5
	%.5 =l phi @cond_true.3 0, @cond_false.4 %.3
	storel %.5, %.2
	%.6 =l loadl %.2
	%.7 =w loadw %.6
	ret %.7
}
